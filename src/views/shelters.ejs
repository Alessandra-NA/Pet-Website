<html>
    <body>
        <p class="h1">Refugios</p>
        <div class="row row-cols-1 row-cols-md-5 g-4" style="margin-top: 1rem;">
            <% refugios.forEach(function(refugio){ %>
                <div class="col">
                    <div class="card" role="button" onclick="location.href='./shelter_detalle?id=<%=refugio.id%>'">
                        <img src="data:image/png;base64,<%=refugio.photo?refugio.photo:''%>" class="img-fluid card-img-top" alt="...">
                        <div class="card-body"> 
                            <h5 class="card-title"><p class="h5" style="margin-top: .5rem; margin-bottom: .5rem;"><%=refugio.name%></p></h5>
                            <p class="card-text">
                                <div class="row" style="margin-left: auto;">                              
                                    <%if(refugio.location === null){%>
                                        None
                                    <% } else{%>
                                        <%=refugio.location.country%>, <%=refugio.location.province%>, <%=refugio.location.district%>
                                    <% } %> 
                                </div>
                                <div class="row">
                                    <div class="d-flex justify-content-center ">
                                        <% for (var i =0; i<refugio.rating;i++) {%> 
                                            <img src='data:image/png;base64,' width="40rem" height="40rem" >
                                        <% } %>
                                        <% for (var i =0; i<(5-refugio.rating);i++) {%> 
                                            <img src='https://us.123rf.com/450wm/stockgiu/stockgiu1911/stockgiu191113891/133793719-estrella-icono-de-estilo-de-l%C3%ADnea-de-cinco-puntas.jpg?ver=6' width="40rem" height="40rem" >
                                        <% } %>
                                    </div>
                                </div>        
                            </p>
                        </div>
                    </div>
                </div>
            <% }) %>
        </div>
    </body>
</html>


